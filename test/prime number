public class prime {
    public static void main(String[] args){
        int n=50;
        System.out.println("Prime numbers up to"+n+":");
        for(int i=2;i<=n;i++){
            if(isprime(i)){
        }
    }
}
private static boolean isprime (int number) {
    for (int j = 2; j < number; j++) {
        if (number % j == 0) {
            return false;
        }
    }
    return true;
   }
}
