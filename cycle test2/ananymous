import java.util.*;
public class anonymous {
    public static void main(String[] args) {
        int[] numbers = new int[]{1, 2, 3, 4, 5};
        printArray(new int[]{10, 20, 30});
    }
    static void printArray(int[] arr) {
        for (int num : arr) {
            System.out.print(num + " ");
        }
        System.out.println();
    }
}
